#: import AKCircularProgress akivymd.uix.progresswidget
# import AKCircularProgress Power_App_cls.progresswidget

<Supervisor_menu>:
    MDBoxLayout:
        size_hint: 1,.94
        MDGridLayout:
            size_hint: .5,.5
            pos_hint: {"top":1, "left":1}
            cols: 2
            MDSlider:
                min: 0
                max: 100
                value: 0
                on_active: progress_percent.current_percent = self.value

                #spacing: dp(30)
        AKCircularProgress:
            id: progress_percent
            pos_hint: {"center_x": .5, "center_y": .5}
            size_hint: None, None
            size: dp(30), dp(30)
            percent_size: dp(7)
            line_width: dp(1)
            percent_type: "relative"
            start_deg: 180
            end_deg: 540
